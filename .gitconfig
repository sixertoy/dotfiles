[alias]
	st = status
	co = checkout
	br = branch
	rbi = rebase -i
	amend = commit --amend
	aamend = commit --amend --no-edit
	rewind = reset HEAD~1
	cancel-rewind = reset HEAD@{1}
	unstage = reset HEAD --
	desc = branch --edit-description
	infos = remote show origin
	tree = log --decorate --graph
	changed = show --name-status
	undo = checkout --
	whoami = config --get user.name
	llog = log -1
	cr = cherry-pick
	mg = merge --no-ff
	plr = pull --rebase
[color]
	ui = true
[core]
	editor = atom --wait
	autocrlf = input
	encoding = utf8
	whitespace = -trailing-space
	excludesFile = /Usr/matthieu/.gitignore
[diff]
	mnemonicPrefix = true
[format]
	pretty = %C(green)%ad %an %C(yellow)%h\n%Creset%B
[gui]
	encoding = utf-8
[log]
	date = format:%a %v %T
[pull]
	rebase = preserve
[push]
	default = simple
	follwTags = true
[tag]
	sort = version:refname
[user]
	name = xxxx
	email = xxxxxxxx
